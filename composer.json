  "homepage": "https://github.com/bayfrontmedia/multi-logger",
  "license" : "MIT",
  "type": "library",
  "keywords": [
    "php",
    "log",
    "logger",
    "monolog"
  ],
  "authors": [
    {
      "name": "John Robinson",
      "email": "john@bayfrontmedia.com",
      "homepage": "https://www.bayfrontmedia.com"
    }
  ],
  "support": {
    "issues": "https://github.com/bayfrontmedia/multi-logger/issues"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "require": {
    "php": "^8.0",
    "monolog/monolog": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Bayfront\\MultiLogger\\": "src/"
    }
  }
}